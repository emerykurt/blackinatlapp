<%= form_for "rating", :url => { :controller => "ratings", :action => "create" }, :html => {:method => :post} do |f| %>
  <%= f.hidden_field :eatery_id, value: @eatery.id %>
    <%= f.label :Expensive %>:
    <%= f.check_box :price_point %><br/>
    <%= f.label :Customer_Service %>:
    <%= f.number_field :customer_service, within: 0...11 %><br/>
    <%= f.label :Product_Quality %>:
    <%= f.number_field :product_quality, within: 0...11 %><br/>
    <%= f.label :Checkout_Experience %>:
    <%= f.number_field :checkout_experience, within: 0...11 %><br/>
    <%= f.label :Overall_Experience %>:
    <%= f.number_field :overall_experience, within: 0...11 %><br/><br/>
    <%= f.submit :submit %>    
<% end %>
