
<h2><%= current_user.first_name %>'s Profile</h2>

<p>Username: @<%= current_user.username %></p> 
<p>First Name: <%= current_user.first_name %></p>
<p>Last Name: <%= current_user.last_name %></p>
<p>Email: <%= current_user.email %></p>

<%= link_to"<button>Edit Profile</button>".html_safe, edit_user_path(current_user)%>
<%= button_to("Delete Profile", user_path(current_user), method: 'delete')%>
